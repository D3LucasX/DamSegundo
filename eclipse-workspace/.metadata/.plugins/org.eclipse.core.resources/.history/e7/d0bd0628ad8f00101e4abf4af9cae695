package Ejercicios_Iniciaci√≥n;

import java.lang.reflect.Array;

public class ejercicio3 {

	public static void main(String[] args) {
		int[] numeros = new int[6];
		for(int i = 0; i <= numeros.length; i++) {
			numeros[i] = i +1;
		}
		for (int i = 0; i<numeros.length; i++) {
			if(numeros[i] % 2 == 0) {
				System.out.println(numeros[i]);
			}else {
				continue;
			}
		}

	}

}
